{
  "entryPointStrategy": "Expand",
  "entryPoints": "./src",
  "excludeExternals": true,
  "excludeInternal": false,
  "externalPattern": [
    "**/node_modules/**",
    "**/tests/**",
    "**/dist/**",
    "**/.*.spec.ts",
    "**/.*.e2e.ts"
  ],
  "internalNamespace": "Internals",
  "darkHighlightTheme": "dark-plus",
  "out": "./docs",
  "logLevel": "Verbose"
}