{
  "name": "medusa-extender",
  "version": "1.0.8-alpha.0",
  "description": "Extends functionalities from medusajs\n\n",
  "keywords": [
    "medusa",
    "medusajs",
    "e-commerce",
    "ecommerce",
    "headless",
    "extension",
    "nodejs"
  ],
  "author": "adrien2p <adrien.deperetti@gmail.com>",
  "homepage": "https://github.com/adrien2p/medusa-extender",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": false,
  "files": [
    "dist",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/adrien2p/medusa-extender"
  },
  "bugs": {
    "url": "https://github.com/adrien2p/medusa-extender/issues"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint src --fix",
    "format": "./node_modules/.bin/prettier src --write",
    "build:ts": "./node_modules/.bin/swc src -d ./dist --config-file .swcrc",
    "build:declarations": "./node_modules/.bin/tsc --emitDeclarationOnly",
    "build": "npm run build:ts && npm run build:declarations",
    "build:doc": "./node_modules/.bin/typedoc",
    "build:graph": "./node_modules/.bin/ts_dependency_graph --filter 'types.ts' --base_path ./src --max_depth 3 --start ./src/medusa.ts | dot -T jpeg > ./assets/medusa-extender.jpeg",
    "test": "NODE_ENV=test ./node_modules/.bin/jest"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "core-js": "^3.20.3",
    "mongoose": "^6.1.6",
    "regenerator-runtime": "^0.13.9"
  },
  "peerDependencies": {
    "@medusajs/medusa": "^1.1.62",
    "awilix": "^6.0.0",
    "express": "^4.17.1",
    "medusa-interfaces": "^1.1.31",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@medusajs/medusa": "^1.1.62",
    "@nestjs/common": "^8.2.3",
    "@swc-node/jest": "^1.4.1",
    "@swc/cli": "^0.1.52",
    "@swc/core": "^1.2.118",
    "@swc/jest": "^0.2.17",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "awilix": "^6.0.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "express": "^4.17.1",
    "jest": "^27.3.1",
    "medusa-interfaces": "^1.1.31",
    "prettier": "^2.5.0",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts_dependency_graph": "^1.2.0",
    "tslint": "~6.1.0",
    "typedoc": "^0.22.10",
    "typedoc-plugin-markdown": "^3.11.7",
    "typedoc-plugin-missing-exports": "^0.22.6",
    "typeorm": "^0.2.41",
    "typescript": "^4.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": ["@swc/jest", { "configFile": ".swcrc.spec" }]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
