{
  "name": "medusa-extender",
  "version": "1.0.19-alpha.5",
  "description": "Extends functionalities from medusajs. Add custom entity fields, override or extend entities, services, repositories, create custom middlewares",
  "keywords": [
    "medusa",
    "medusajs",
    "e-commerce",
    "ecommerce",
    "headless",
    "extension",
    "nodejs"
  ],
  "author": "adrien2p <adrien.deperetti@gmail.com>",
  "homepage": "https://github.com/adrien2p/medusa-extender",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": false,
  "files": [
    "dist",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/adrien2p/medusa-extender"
  },
  "bugs": {
    "url": "https://github.com/adrien2p/medusa-extender/issues"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint src --fix",
    "format": "./node_modules/.bin/prettier src --write",
    "build:ts": "./node_modules/.bin/swc src -d ./dist --config-file .swcrc",
    "build:declarations": "./node_modules/.bin/tsc --emitDeclarationOnly",
    "build": "rm -rf dist && npm run build:ts && npm run build:declarations",
    "build:doc": "rm -rf docs && ./node_modules/.bin/typedoc && cp .github/docs/index.html docs/",
    "build:graph": "./node_modules/.bin/ts_dependency_graph --filter 'types.ts' --base_path ./src --max_depth 4 --start ./src/index.ts | dot -T jpeg > ./assets/medusa-extender.jpeg",
    "serve:doc": "./node_modules/.bin/docsify serve docs",
    "test": "NODE_ENV=test ./node_modules/.bin/jest -c jest.config.js"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "mongoose": "^6.1.6"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.1.13",
    "@medusajs/medusa": "^1.1.62",
    "awilix": "^6.0.0",
    "express": "^4.17.1",
    "medusa-interfaces": "^1.1.31",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@medusajs/medusa": "^1.1.62",
    "@swc/cli": "^0.1.52",
    "@swc/core": "^1.2.118",
    "@swc/jest": "^0.2.17",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "awilix": "^6.0.0",
    "core-js": "^3.20.3",
    "docsify": "^4.12.2",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "express": "^4.17.1",
    "jest": "^27.3.1",
    "medusa-interfaces": "^1.1.31",
    "prettier": "^2.5.0",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "^0.13.9",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts_dependency_graph": "^1.2.0",
    "tslint": "~6.1.0",
    "typedoc": "^0.22.10",
    "typedoc-plugin-markdown": "^3.11.7",
    "typedoc-plugin-missing-exports": "^0.22.6",
    "typeorm": "^0.2.41",
    "typescript": "^4.5.2"
  }
}
